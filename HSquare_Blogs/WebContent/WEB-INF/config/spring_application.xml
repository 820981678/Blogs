<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<bean id="dbHandle" class="com.connection.db.DBHandle" depends-on="propertiesConfigurer" lazy-init="false" singleton="true" init-method="init">
		<property name="configNames">
			<set>
				<value>default</value>
			</set>
		</property>
		<property name="dbProperties" ref="propertiesConfigurer" />
	</bean>
	
	<bean id="globalCache" class="com.glogs.init.cache.GlobalCache">
		<property name="cacheInit">
			<list>
				<bean id="bTag" class="com.glogs.init.cache.inter.impl.BTagInitImpl"></bean>
			</list>
		</property>
	</bean>
	
	<!-- 静态页面生成服务 -->
	<bean id="blogStatic" class="com.generate.BlogStatic"></bean>
	
</beans>